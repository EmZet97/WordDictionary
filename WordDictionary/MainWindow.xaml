<Window x:Class="WordDictionary.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WordDictionary"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" ResizeMode="NoResize">
    <StackPanel>
        <TextBox Name="InputBox" Margin="200 50 200 0" Padding="5" TextChanged="InputBox_TextChanged"></TextBox>
        <ComboBox Name="DropDownBox" Margin="200 10 200 0"></ComboBox>
        <Button IsEnabled="False" x:Name="CreateButton" Content="Dodaj słowo" Padding="2" Margin="250 10 250 10" Click="CreateButton_Click"></Button>
        <TextBlock x:Name="test"></TextBlock>
    </StackPanel>
</Window>
